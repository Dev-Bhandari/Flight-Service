{
    "name": "airline-booking-system",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "node --watch src/index.js",
        
        "mig:gen": "cd src && npx sequelize-cli migration:generate",
        "mig:run": "cd src && npx sequelize-cli db:migrate",
        "mig:undo": "cd src && npx sequelize-cli db:migrate:undo",
        "mig:undo:all": "cd src && npx sequelize-cli db:migrate:undo:all",

        "model:gen": "cd src && npx sequelize-cli model:generate",
        "seed:gen": "cd src && npx sequelize-cli seed:generate",
        "seed:run": "cd src && npx sequelize-cli db:seed:all",
        "seed:undo": "cd src && npx sequelize-cli db:seed:undo",
        "seed:undo:all": "cd src && npx sequelize-cli db:seed:undo:all"
    },
    "author": "Dev Bhandari",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "http-status-codes": "^2.3.0",
        "mysql2": "^3.15.3",
        "sequelize": "^6.37.7",
        "sequelize-cli": "^6.6.3",
        "winston": "^3.18.3"
    }
}
